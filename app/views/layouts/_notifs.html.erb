<% flash.each do |key, value| %>
	<% unless key.to_s == "timedout" %>
			<% if key == "notice" %>
        		<div class="alert alert-success">
      		<% elsif key == "alert" %>
        		<div class="alert alert-danger">
        	<% else %>
    			<div class="alert alert-<%= key %>">
    		<% end %>
    			
    		<%= value %></div>
    <% end %>
<% end %>